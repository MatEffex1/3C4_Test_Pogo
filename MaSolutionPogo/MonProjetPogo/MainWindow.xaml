<Window x:Class="MonProjetPogo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MonProjetPogo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" FontSize="50">
    <StackPanel x:Name="StackPanelMain">
        <WrapPanel>
            <TextBlock Width="300">Name</TextBlock>
            <TextBox Width="300" Background="Gray" x:Name="TextBoxName">Enter text here</TextBox>
        </WrapPanel>
        <TextBlock x:Name="TextBlockGame">Text</TextBlock>
        <Button x:Name="ButtonClickMe">Click here</Button>
    </StackPanel>
</Window>
